                    <div class="panel-body">
                        <div class="canvas-wrapper">
                            <canvas class="main-chart" id="line-chart" height="200" width="600"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>



        <!--/.row stat avec nombre d event pour chaque categorie-->

    <div class="row">
        <div class="col-xs-6 col-md-3">
            <div class="panel panel-default">
                <div class="panel-body easypiechart-panel">
                    <h4>Evenmenets du mois</h4>
                    <div class="easypiechart" id="easypiechart-blue" data-percent="{{ eventCent }}"><span class="percent">{{ eventCent }}%</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-6 col-md-3">
